{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "DataBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "j40-sit-justice40-data-harvester-data",
        "AccessControl": "PublicRead",
        "CorsConfiguration": {
          "CorsRules": [
            {
              "AllowedOrigins": [
                "*"
              ],
              "AllowedMethods": [
                "GET"
              ],
              "AllowedHeaders": [
                "Content-Length"
              ]
            }
          ]
        },
        "WebsiteConfiguration": {
          "IndexDocument": "index.html",
          "ErrorDocument": "error.html"
        }
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": "j40-sit-us-east-1-justice40-data-harvester",
      "Export": {
        "Name": "sls-justice40-data-harvester-sit-ServerlessDeploymentBucketName"
      }
    }
  }
}